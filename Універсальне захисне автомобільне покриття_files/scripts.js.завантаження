$(document).ready((function(){function t(t){return"<span>"+(t=("00"+t).substr(-2))[0]+"</span><span>"+t[1]+"</span>"}$("a[href^='#']").click((function(){var t=$(this).attr("href");return $("html, body").animate({scrollTop:$(t).offset().top+"px"}),!1})),function e(){var o=new Date,s=new Date;s.setHours(23),s.setMinutes(59),s.setSeconds(59),23===o.getHours()&&59===o.getMinutes()&&59===o.getSeconds&&s.setDate(s.getDate()+1);var r=Math.floor((s.getTime()-o.getTime())/1e3),a=Math.floor(r/3600);r-=3600*a;var n=Math.floor(r/60);r-=60*n,$(".timer .hours").html(t(a)),$(".timer .minutes").html(t(n)),$(".timer .seconds").html(t(r)),setTimeout(e,200)}(),$(".owl-carousel").owlCarousel({items:1,loop:!0,smartSpeed:300,mouseDrag:!1,pullDrag:!1,dots:!1,nav:!0,navText:""})}));